%% \newdefinition{#1 term}{#2 label}{#3 shortdef}{#4 longdef}
\newcommand\newdefinition[4]{%
\nomenclature{#1}{#3\hfill Definition~\ref{#2}, page~\pageref{#2}.}%
\begin{definition}%
\label{#2}%
{#4}%
\end{definition}%
}

%% \newnotation{#1 term}{#2 label}{#3 shortdef}{#4 longdef}
\newcommand\newnotation[4]{%
\nomenclature{#1}{#3\hfill Notation~\ref{#2}, page~\pageref{#2}.}%
\begin{notation}%
\label{#2}%
{#4}%
\end{notation}%
}

%% \figurehere{#1 label}{#2 caption}{#3 content}
\newcommand\figurehere[3]{%
{\begin{minipage}{\textwidth}% see https://tex.stackexchange.com/questions/30734/how-make-sure-two-elements-stay-on-the-same-page
\begin{center}
{#3}
\captionsetup{type=figure}% see https://tex.stackexchange.com/questions/32344/issue-with-capt-of-package
\captionof{figure}{#2}
\label{#1}
\end{center}
\end{minipage}
}
}

%% \nofigurehere{#1 label}{#2 caption}{#3 content}
\newcommand\nofigurehere[1]{%
{\begin{minipage}{\textwidth}
\begin{center}
{#1}
\end{center}
\end{minipage}
}
}



%% \figurefloat{#1 placement}{#2 label}{#3 caption}{#4 content}
\newcommand\figurefloat[4]{%
\begin{figure}[#1]
\centering
{#4}
\caption{#3}
\label{#2}
\end{figure}
}

